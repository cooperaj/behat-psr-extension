{
  "name": "cooperaj/behat-psr-extension",
  "description": "Provides a bootstrapped PSR7 application and PSR11 container to your behat contexts",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Cooper",
      "email": "adam@acpr.dev"
    }
  ],
  "require": {
    "php": "^7.2",
    "behat/mink-browserkit-driver": "^1.3.3",
    "behat/mink-extension": "^2.3",
    "symfony/http-foundation": "^4.3 || ^5.1",
    "symfony/psr-http-message-bridge": "^1.2",
    "symfony/http-kernel": "^4.3 || ^5.1"
  },
  "autoload": {
    "psr-4": {
      "Acpr\\Behat\\Psr\\": "src/",
      "Acpr\\Behat\\Psr\\FeatureContexts\\": "tests/features/contexts/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TestAcpr\\Behat\\Psr\\": "tests/unit"
    }
  },
  "require-dev": {
    "laminas/laminas-diactoros": "^2.2",
    "laminas/laminas-config-aggregator": "^1.1",
    "laminas/laminas-servicemanager": "^3.4",
    "mezzio/mezzio": "^3.2",
    "mezzio/mezzio-fastroute": "^3.0",
    "behat/behat": "^3.5",
    "phpunit/phpunit": "^8.0",
    "vimeo/psalm": "^3.6"
  }
}
