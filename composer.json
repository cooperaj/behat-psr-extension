{
  "name": "cooperaj/behat-psr-extension",
  "description": "Provides a bootstrapped PSR7 application and PSR11 container to your behat contexts",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Cooper",
      "email": "adam@acpr.dev"
    }
  ],
  "require": {
    "php": "^7.1",
    "behat/mink-browserkit-driver": "1.3.3",
    "behat/mink-extension": "^2.3",
    "symfony/http-foundation": "^4.3",
    "symfony/psr-http-message-bridge": "^1.2",
    "symfony/http-kernel": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Acpr\\Behat\\Psr\\": "src/",
      "Acpr\\Behat\\Psr\\FeatureContexts\\": "features/contexts/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TestAcpr\\Behat\\Expressive\\": "tests/"
    }
  },
  "require-dev": {
    "zendframework/zend-diactoros": "^2.2",
    "zendframework/zend-expressive": "^3.2",
    "zendframework/zend-config-aggregator": "^1.1",
    "zendframework/zend-servicemanager": "^3.4",
    "zendframework/zend-expressive-fastroute": "^3.0",
    "behat/behat": "^3.5",
    "phpunit/phpunit": "^8.4",
    "vimeo/psalm": "^3.6"
  }
}
